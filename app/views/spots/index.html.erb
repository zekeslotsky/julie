<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

<div class="container">
  <h1>Hi Julie!</h1>
   <br><h2>Today we had fun with the Google Maps API</h2>
  
<div style='width: 800px;'>
  <div id="multi_overlays" style='width: 800px; height: 400px;'></div>
</div>


  <script type="text/javascript">
var handler = Gmaps.build('Google');
handler.buildMap({ internal: {id: 'multi_overlays'}}, function(){


  var circles = handler.addCircles(
    [{ lat: 34.052525, lng: -118.47, radius: 50 }],
    { strokeColor: '#FF0000'}
  );

  var polylines = handler.addPolylines(
    [
      [
        {lat: 34.047, lng: -118.47},
        {lat: 34.050525, lng: -118.47}
      ]
    ],
    { strokeColor: '#FF0000'}
  );

  var polylines = handler.addPolylines(
    [
      [
        {lat: 34.047, lng: -118.45},
        {lat: 34.053, lng: -118.45}
      ]
    ],
    { strokeColor: '#FF0000'}
  );

    var polylines = handler.addPolylines(
    [
      [
        {lat: 34.047, lng: -118.45},
        {lat: 34.047, lng: -118.455}
      ]
    ],
    { strokeColor: '#FF0000'}
  );

    var polylines = handler.addPolylines(
    [
      [
        {lat: 34.047, lng: -118.455},
        {lat: 34.053, lng: -118.455}
      ]
    ],
    { strokeColor: '#FF0000'}
  );


  var polygons = handler.addPolygons(
    [
      [ 
        {lat:  34.052, lng:  -118.466}, { lat: 34.055, lng:  -118.465},          
        {lat:  34.0525, lng:  -118.463}, { lat: 34.055, lng:  -118.461},  
        {lat:  34.052, lng:  -118.460}, { lat: 34.047, lng:  -118.463},
      ]
    ],
    { strokeColor: '#FF0000'}
  );

  handler.bounds.extendWith(polylines);
  handler.bounds.extendWith(polygons);
  handler.bounds.extendWith(circles);
  handler.fitMapToBounds();
});

   </script>
 </div>
